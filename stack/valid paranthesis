paranthesis = input('Enter the paranthesis: ')
def check_paranthesis(paranthesis):
    st=[]
    for char in paranthesis:
        if char == '(' or char== '{' or char == '[':
            st.append(char)
        elif char ==')':
            if not st or st[-1]!='(':
                return False
            st.pop()
        elif char =='}':
            if not st or st[-1]!='{':
                return False
            st.pop()
        elif char ==']':
            if not st or st[-1]!='[':
                return False
            st.pop()
        else:
            return False
            
    return len(st)==0
            
print(check_paranthesis(paranthesis))
            
    
